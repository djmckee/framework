{% extends 'page.twig' %}

{% block header %}
    <div class="col-md-12">
	<h1>Developer tools</h1>
    </div>
{% endblock header %}

{% block main %}
    <div class="row">
	{% include 'message.twig' %}
        {% if errlog is defined %}
            <pre>
                {{errlog}}
            </pre>
        {% else %}
            <div class="col-md-offset-2 col-md-8">
                <p>Select a developer action from the navigation bar.</p>
            </div>
        {% endif %}
    </div>
{% endblock main %}

{% block pagefooter %}
{# I don't want a footer #}
{% endblock pagefooter %}
